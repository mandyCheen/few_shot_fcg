{
    "dataset": {
        "pack_filter": "diec",
        "cpu_arch": "x86_64",
        "raw": "malware_diec_x86_64_fcg_dataset.csv",
        "split_by_cpu": false
    },
    "settings":{
        "model": {
            "model_name": "GraphSAGE",
            "input_size": 128,
            "hidden_size": 128,
            "output_size": 128
        },
        "train": {
            "training": true,
            "validation": true,
            "num_epochs": 100,
            "device": "cuda:0",
            "parallel": false,
            "parallel_device": [0, 1],
            "iterations": 100,
            "lr": 0.001,
            "lr_scheduler": {
                "use": true,
                "method": "StepLR",
                "step_size": 20,
                "gamma": 0.5
            },
            "loss": "distances",
            "optimizer": "Adam",
            "save_model": true
        },
        "few_shot": {
            "method": "ProtoNet",
            "train":{
                "support_shots": 5,
                "query_shots": 5,
                "class_per_iter": 20
            },
            "test":{
                "support_shots": 5,
                "query_shots": 15,
                "class_per_iter": 5
            }
        },
        "vectorize": {
            "node_embedding_method": "word2vec",
            "node_embedding_size": 128,
            "num_workers": 24
        },
        "seed": 42
    },
    "paths": {
        "data": {
            "fcg_dataset": "./data_5_9_4_fcg",
            "csv_folder": "./dataset/raw_csv",
            "split_folder": "./dataset/split",
            "model_folder": "./models",
            "embedding_folder": "./embeddings"
        }
    }

}