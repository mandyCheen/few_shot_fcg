{
    "dataset": {
        "pack_filter": "diec",
        "cpu_arch": "x86_64",
        "reverse_tool": "ghidra",
        "raw": "malware_diec_ghidra_x86_64_fcg_dataset.csv",
        "split_by_cpu": false,
        "pretrain_family": [
            "gafgyt",
            "ngioweb",
            "mirai",
            "tsunami"
        ]
    },
    "pretrain": {
        "name": "x86_pretrained_GCN",
        "use": true,
        "raw_dataset": "malware_diec_ghidra_x86_64_fcg_pretrain_dataset.csv",
        "batch_size": 128
    },
    "settings": {
        "name": "x86_few_shot",
        "model": {
            "model_name": "GCN",
            "input_size": 128,
            "hidden_size": 128,
            "output_size": 128,
            "num_layers": 2,
            "projection": true,
            "load_weights": "/home/mandy/Projects/few_shot_fcg/pretrained/x86_pretrained_GCN_20241201_1928/epoch_2112_best.pth"
        },
        "train": {
            "training": true,
            "validation": true,
            "num_epochs": 5000,
            "device": "cuda:0",
            "parallel": false,
            "parallel_device": [],
            "iterations": 100,
            "lr": 0.01,
            "projection_lr": 0.01,
            "lr_scheduler": {
                "use": true,
                "method": "ReduceLROnPlateau",
                "step_size": 20,
                "gamma": 0.5,
                "patience": 5,
                "factor": 0.5
            },
            "early_stopping": {
                "use": true,
                "patience": 50
            },
            "loss": "CrossEntropyLoss",
            "distance": "cosine",
            "optimizer": "Adam",
            "save_model": true
        },
        "few_shot": {
            "method": "ProtoNet",
            "train": {
                "support_shots": 5,
                "query_shots": 15,
                "class_per_iter": 5
            },
            "test": {
                "support_shots": 5,
                "query_shots": 15,
                "class_per_iter": 5
            }
        },
        "vectorize": {
            "node_embedding_method": "word2vec",
            "node_embedding_size": 128,
            "num_workers": 24
        },
        "seed": 42
    },
    "paths": {
        "data": {
            "fcg_dataset": "../dataset/data_ghidra_fcg",
            "csv_folder": "../dataset/raw_csv",
            "split_folder": "../dataset/split",
            "embedding_folder": "/mnt/ssd2t/mandy/Projects/few_shot_fcg/embeddings",
            "pretrain_dataset": "../dataset/data_ghidra_fcg_pretrain"
        },
        "model": {
            "model_folder": "../models",
            "pretrained_folder": "../pretrained"
        }
    }
}